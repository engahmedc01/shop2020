<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>  
<class name="App.core.beans.Outcome" table="OUTCOME">  
      <id name = "id" type = "int" column = "ID">
          <generator class="increment"></generator>  
          </id>  
            <property name = "outcomeDate" column = "OUTCOME_DATE" />
               <property name = "totalOutcome" column = "TOTAL_OUTCOME" />
             	<property name = "seasonId" column = "SEASON_ID" />
             	             	<property name = "safeId" column = "SAFE_ID" />
             	
               
        <property name = "changed" column = "CHANGED" />
               <property name = "changeDate" column = "CHANGE_DATE" />
               <property name = "timestamp" column = "TIMESTAMP" />
               <property name = "changerId" column = "CHANGER_ID" />
            
         
</class>  
            
            
            

            
</hibernate-mapping>  